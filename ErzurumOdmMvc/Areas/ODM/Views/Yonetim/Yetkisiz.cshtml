
@{
    ViewBag.Title = "401 Unauthorized";
}
<div class="content-wrapper">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">@ViewBag.Title</h1>
                </div>
            </div>
        </div>
    </section>
    <section class="content">
        <div class="error-page">
            <h2 class="headline text-warning"> 401</h2>

            <div class="error-content">
                <h3><i class="fas fa-exclamation-triangle text-warning"></i> Unauthorized.</h3>

                <p>
                    Erişim yetkiniz bulunmamaktadır.<br/> <a href="@Url.Action("Index")">Kontrol Paneli</a>
                </p>

            </div>
        </div>
    </section>
</div>
